            @Html.DisplayFor(modelItem => item.PrimaryEmail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SecondaryEmail)
        </td>

        <td>
            @Html.ActionLink("Edit", "Edit", new {controller="parents",area="Admin", parentId=item.ParentsId }) |
            @Html.ActionLink("Details", "Details", new { controller = "parents", area = "Admin", parentId = item.ParentsId }) |
           
        </td>
    </tr>
}

</table>
