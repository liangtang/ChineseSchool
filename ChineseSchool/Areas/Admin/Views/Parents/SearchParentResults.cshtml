@model IEnumerable<ChineseSchool.Entities.Parent>

@{
    ViewBag.Title = "SearchParentResults";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>SearchParentResults</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        
        <th>
            @Html.DisplayNameFor(model => model.Parent1Firstname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Parent1LastName)
