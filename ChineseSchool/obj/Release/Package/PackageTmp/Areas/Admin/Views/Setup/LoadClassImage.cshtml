@model System.Int32
@{
    ViewBag.Title = "LoadClassImage";
}

<h2>LoadClassImage</h2>

@using (Html.BeginForm("LoadClassImage", "Setup", FormMethod.Post, new { enctype = "multipart/form-data" }))
{ 
    @Html.Hidden("classId",Model)
<div class="form-horizontal">
    <div class="form-group">
        <label for="file" class="control-label col-md-2">Filename:</label>
      
        <div class="col-md-10">
            <input type="file" name="file" />
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Submit" class="btn btn-default" />
        </div>
    </div>

</div>

}